    "pixi-viewport": "^5.0.1",
    "pixi.js": "^7.2.4",
    "simplex-noise": "^4.0.1",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/types": "^7.21.5",
    "@types/heap": "^0.2.31",
    "@types/md5": "^2.3.2",
    "@vitejs/plugin-vue": "^4.2.1",
    "binpacking": "^0.0.1",
    "jimp": "^0.22.7",
    "jsdom": "^21.1.1",
    "sass": "^1.62.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vitest": "^0.30.1",
