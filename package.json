{
  "name": "open-td",
  "version": "0.0.4",
  "license": "GPL-3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn check-types && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "check-types": "vue-tsc --noEmit",
    "mask-generator": "ts-node-esm -T ./scripts/maskGenerator.ts",
    "wall-generator": "ts-node-esm -T ./scripts/wallGenerator.ts",
    "base-generator": "ts-node-esm -T ./scripts/baseGenerator.ts",
    "atlas-packer": "ts-node-esm -T ./scripts/atlasPacker.ts",
    "install": "yarn wall-generator && yarn base-generator && yarn atlas-packer"
  },
  "dependencies": {
    "@pixi/sound": "5.0.0",
    "alea": "^1.0.1",
    "heap": "^0.2.7",
    "md5": "^2.3.0",
    "pixi-viewport": "^5.0.0",
    "pixi.js": "^7.1.3",
    "simplex-noise": "^4.0.1",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@babel/types": "^7.21.2",
    "@types/heap": "^0.2.31",
    "@types/jest": "^29.2.4",
    "@types/md5": "^2.3.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "binpacking": "^0.0.1",
    "jest-environment-jsdom": "^29.4.3",
    "jimp": "^0.22.7",
    "sass": "^1.58.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.1.4",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  }
}
